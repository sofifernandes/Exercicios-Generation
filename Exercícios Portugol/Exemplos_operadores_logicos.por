programa
{
	funcao inicio()
	{
		inteiro a,b
		a=5
		b=10
		escreva("O valor de A: ", a, " é maior que o valor de B:", b)
		escreva("\nA é maior que B? ", a>b)
		escreva("\nO valor de A: ", a, " é menor que o valor de B:", b)
		escreva("\nA é menor que B? ", a<b)
		escreva("\nO valor de A: ", a, " é diferente que o valor de B:", b)
		escreva("\nA é diferente de B?", a!=b)
		escreva("\nO valor de A: ", a, " é igual que o valor de B:", b)
		escreva("\nA é igual a B?", a==b)
	}
}
